config_version=4

[applicanametion]

config/Misplaced=false

[application]

config/name="Polaris"
run/main_scene="res://Menus/Splash.tscn"
config/use_custom_user_dir=true
boot_splash/image="res://UI/PolarisCover.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0.0117647, 0.0352941, 0.101961, 1 )
config/icon="res://icon.png"
config/windows_native_icon="res://icon.ico"

[autoload]

Utils="*res://Controllers/Utils.gd"
Events="*res://Controllers/Events.gd"
SFX="*res://Music and Sounds/SFX.tscn"
Game="*res://Controllers/Game.tscn"
TransitionCover="*res://Controllers/TransitionCover.tscn"
MainInstances="*res://Controllers/MainInstances.gd"
Global="*res://Controllers/Global.gd"
Music="*res://Controllers/Music.tscn"

[display]

window/size/width=640
window/size/height=480
window/size/test_width=640
window/size/test_height=480
window/stretch/mode="2d"
window/stretch/aspect="keep"

